    "start-prod": "cross-env NODE_ENV=production node ./server/server.js",
    "wds": "webpack-dev-server --content-base public/ --inline",
    "remove-source-map": "rm ./public/bundle.js.map",
    "remove-bundles": "rm ./public/*.js",
    "heroku-postbuild": "cross-env NODE_ENV=production webpack --config ./webpack.prod.js -p"
  },
